<script>
    const icon = document.getElementById('hamburgare-icon');
    const menu = document.getElementById('hamburgare-menu');
    const overlay = document.getElementById('hamburgare-overlay');
    const mainLinks = document.querySelectorAll('.hamburgare-main-link[data-overlay]');
    const overlays = document.querySelectorAll('.hamburgare-overlay');
    const backButtons = document.querySelectorAll('#hamburgare-back');
    const mainContent = document.getElementById('hamburgare-main-content');
    const accountBtn = document.getElementById('login-avatar-btn-unique');
    if (accountBtn) {
        accountBtn.addEventListener('click', function(e){
            e.stopPropagation();
            const overlayId = accountBtn.getAttribute('data-overlay');
            const overlayPanel = document.getElementById(overlayId);
            overlayPanel.style.display = 'block';
            setTimeout(() => overlayPanel.classList.add('active'), 10);
            mainContent.style.display = 'none';
        });
    }
    icon.addEventListener('click', (e) => {
        e.stopPropagation();
        menu.classList.toggle('open');
        overlay.classList.toggle('active');
        mainContent.style.display = 'block';
    });
    overlay.addEventListener('click', () => {
        menu.classList.remove('open');
        overlay.classList.remove('active');
        overlays.forEach(o => {
            o.classList.remove('active');
            setTimeout(() => o.style.display = 'none', 300);
        });
        mainContent.style.display = 'block';
    });
    document.addEventListener('click', (e) => {
        if (!menu.contains(e.target) && e.target !== icon && menu.classList.contains('open')) {
            menu.classList.remove('open');
            overlay.classList.remove('active');
            overlays.forEach(o => {
                o.classList.remove('active');
                setTimeout(() => o.style.display = 'none', 300);
            });
            mainContent.style.display = 'block';
        }
    });
    mainLinks.forEach(link => {
        link.addEventListener('click', (e) => {
            e.stopPropagation();
            const overlayId = link.getAttribute('data-overlay');
            const overlayPanel = document.getElementById(overlayId);
            overlayPanel.style.display = 'block';
            setTimeout(() => overlayPanel.classList.add('active'), 10);
            mainContent.style.display = 'none';
        });
    });
    backButtons.forEach(button => {
        button.addEventListener('click', (e) => {
            e.stopPropagation();
            const overlayPanel = button.closest('.hamburgare-overlay');
            overlayPanel.classList.remove('active');
            setTimeout(() => {
                overlayPanel.style.display = 'none';
                mainContent.style.display = 'block';
            }, 300);
        });
    });
    menu.addEventListener('scroll', () => {
        const header = document.getElementById('hamburgare-header');
        const overlayHeaders = document.querySelectorAll('#hamburgare-overlay-header');
        if (menu.scrollTop > 0) {
            header.classList.add('scrolled');
            overlayHeaders.forEach(overlayHeader => overlayHeader.classList.add('scrolled'));
        } else {
            header.classList.remove('scrolled');
            overlayHeaders.forEach(overlayHeader => overlayHeader.classList.remove('scrolled'));
        }
    });
</script>
</html>
